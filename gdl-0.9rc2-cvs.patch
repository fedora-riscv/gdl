+  template<class T> inline void Nan2One(T& value)
+  { if (!isfinite(value)) value = 1; }
+  template<class T> inline void Nan2OneCpx(T& value)
+  {
+    value = T(isfinite(value.real())? value.real() : 1, 
+              isfinite(value.imag())? value.imag() : 1);
