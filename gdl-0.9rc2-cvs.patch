+	      }
+	    else // s1 and s2 are arrays
+	      {
+		if( l2Scalar)
+		  if( s0->N_Elements() <= s1->N_Elements())
+		    {
+		      res = new DByteGDL( s0->Dim(), BaseGDL::NOZERO);
