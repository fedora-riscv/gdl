+  string TagName( EnvT* e, const string& name)
+  {
+    string n = StrUpCase( name);
+    SizeT len = n.size();
+    if( n[0] != '_' && (n[0] < 'A' || n[0] > 'Z'))
+      e->Throw( "Illegal tag name: "+name+".");
+    for( SizeT i=1; i<len; ++i)
+      {
+	if( n[i] == ' ')
+	  n[i] = '_';
+	else 
+	  if( n[i] != '_' && n[i] != '$' && 
+	      (n[i] < 'A' || n[i] > 'Z') &&
+	      (n[i] < '0' || n[i] > '9'))
+	    e->Throw( "Illegal tag name: "+name+".");
+      }
+    return n;
+  }
