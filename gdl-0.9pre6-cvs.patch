+pro TEST_GREEK_LETTERS, postscript=postscript
+;
+if KEYWORD_SET(postscript) then begin
+   set_plot,'ps'
+   defsysv, '!gdl', exist=exist
+   if (exist EQ 1) then suffix='GDL.ps' else suffix='IDL.ps'
